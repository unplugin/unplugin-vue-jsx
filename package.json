{
  "name": "unplugin-vue-jsx",
  "version": "0.1.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Vue JSX plugin for both Vue 2 and 3. Supports Rollup, Vite, esbuild and Webpack.",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-jsx#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-jsx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-jsx.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-typescript": "^7.18.6",
    "@rollup/pluginutils": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitejs/plugin-vue2-jsx": "^1.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/babel-preset-jsx": "^1.3.0",
    "esbuild": "^0.14.48",
    "local-pkg": "^0.4.1",
    "magic-string": "^0.26.2",
    "unplugin": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@sxzz/eslint-config": "^2.2.2",
    "@types/babel__core": "^7.1.19",
    "@types/node": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "eslint-define-config": "^1.5.1",
    "fast-glob": "^3.2.11",
    "prettier": "^2.7.1",
    "tsup": "^6.1.2",
    "tsx": "^3.4.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
