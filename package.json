{
  "name": "unplugin-vue-jsx",
  "version": "0.2.0",
  "packageManager": "pnpm@8.6.0",
  "description": "Vue JSX plugin for both Vue 2 and 3. Supports Rollup, Vite, esbuild and Webpack.",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-jsx#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-jsx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-jsx.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./cores": {
      "require": "./dist/cores.js",
      "import": "./dist/cores.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-syntax-typescript": "^7.21.4",
    "@rollup/pluginutils": "^5.0.2",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/babel-preset-jsx": "^1.4.0",
    "esbuild": "^0.17.19",
    "local-pkg": "^0.4.3",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^2.4.6",
    "@types/babel__core": "^7.20.1",
    "@types/node": "^20.1.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.42.0",
    "eslint-define-config": "^1.20.0",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "vue": "^3.3.4"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
